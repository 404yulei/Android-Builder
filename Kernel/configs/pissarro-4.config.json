[ 
  {
    "kernelSource": {
      "name": "3RedmiNote11Pro",
      "repo": "https://github.com/404yulei/Xiaomi_Kernel_OpenSource",
      "branch": "pissarro-s-oss",
      "device": "pissarro",
      "defconfig": "tiqu_defconfig"
    },
    "enableCcache": true,
    "toolchains": [
      {
        "repo": "https://gitlab.com/LeCmnGend/proton-clang",
        "branch": "clang-13",
        "name": "clang",
        "binaryEnv": [
          "bin"
        ]
      }
    ],
    "params": {
      "ARCH": "arm64",
      "CC": "clang/bin/clang",
      "externalCommands": {
        "CROSS_COMPILE": "/clang/bin/aarch64-linux-gnu-",
        "CROSS_COMPILE_ARM32": "/clang/bin/arm-linux-gnueabi-",
        
        "CLANG_TRIPLE": "aarch64-linux-gnu-",
        "LD": "ld.lld",
        "AR": "llvm-ar",
        "NM": "llvm-nm",
        "AS": "llvm-as",
        "OBJCOPY": "llvm-objcopy",
        "OBJDUMP": "llvm-objdump",
        
        
        "STRIP": "llvm-strip",
        "LDGOLD": "aarch64-linux-gnu-ld.gold"
        
        
      }
    },
    "AnyKernel3": {
      "use": true,
      "release": true
    },
    "enableKernelSU": false,
    "enableLXC": false
  }
]
